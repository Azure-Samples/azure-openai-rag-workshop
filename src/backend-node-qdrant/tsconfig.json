{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "outDir": "dist",
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "lib": ["esnext"]
  },
  "include": ["src/**/*.ts"],
  "exclude": ["src/**/_*.ts"]
}
